# Supabase Configuration
# Project: PaintMixr
# Get these values from https://app.supabase.com/project/<project-id>/settings/api

NEXT_PUBLIC_SUPABASE_URL=https://your-project-id.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-key-here

# Optional: Service Role Key (for server-side operations - keep this secure!)
# SUPABASE_SERVICE_ROLE_KEY=your_service_role_key_here

# Database Configuration
DATABASE_URL=postgresql://postgres:[YOUR-PASSWORD]@db.your-project-id.supabase.co:5432/postgres

# Environment
NODE_ENV=development

# App Configuration
NEXT_PUBLIC_APP_URL=http://localhost:3000

# Feature Flags (optional)
# NEXT_PUBLIC_ENABLE_ENHANCED_OPTIMIZATION=true
# NEXT_PUBLIC_ENABLE_PAINT_COLLECTIONS=true
# NEXT_PUBLIC_ENABLE_ALGORITHM_SELECTION=true
# NEXT_PUBLIC_ENABLE_PERFORMANCE_MONITORING=true
